<!-- Service Column -->
<div class="col-sm-12 service-single-wrap">
	<!-- Service Wrapper -->
	<div class="row">
		<!-- Service Image Wrapper -->
		<div class="col-md-12">
			{% if  node.field_service_image|length ==1 %}
				<div class="service-img-wrap">
					{% set image_url = getUrlByImageStyle(node.field_service_image.entity.uri.value, 'image_1920x966') %}
					<img src="{{ image_url }}" alt="{{ title }}" class="img-responsive" height="966" width="1920">
				</div>
			{% elseif node.field_service_image|length >1%}
				<div class="service-img-wrap">
					<div class="owl-carousel dots-inline" 
					data-animatein="" data-animateout="" 
					data-items="1" 
					data-margin="" 
					data-loop="true" 
					data-merge="true" 
					data-nav="true" 
					data-dots="true" 
					data-stagepadding="" 
					data-mobile="1" 
					data-tablet="1" 
					data-desktopsmall="1" 
					data-desktop="1" 
					data-autoplay="true" 
					data-delay="3000" 
					data-navigation="true">
						{% for img in node.field_service_image %}
	                	{% set image_url = getUrlByImageStyle(img.entity.uri.value, 'image_1920x966') %}
							<div class="item"><img src="{{ image_url }}" alt="{{ title }}" class="img-responsive" height="966" width="1920"></div>
						{% endfor %}
					</div><!-- Image carousel -->
				</div><!-- Service Image Wrapper -->
			{% endif %}
				
		</div><!-- Service Image Column -->
	</div><!-- Service Wrapper -->
	
	{{ content.body }}
</div><!-- Column -->