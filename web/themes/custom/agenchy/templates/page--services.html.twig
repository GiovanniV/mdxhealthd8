<!-- Page Main -->
<div role="main" class="main">
	<div class="page-default bg-grey typo-dark">
		{% if services_layout == 'grid' %}
			{% if services_sidebar == 'left' %}
				<!-- Container -->
				<div class="container">
					<div class="row">
						<!-- Sidebar -->
						{% if page.services_sidebar %}
							<div class="col-md-3">
								<aside class="sidebar">
									{{ page.services_sidebar }}
								</aside>
							</div><!-- Column -->
						{% endif %}
						<!-- Page Content -->
						<div class="col-md-9">
							{{ page.content }}
						</div><!-- Column -->
					</div><!-- Row -->
				</div><!-- Container -->
			{% elseif services_sidebar == 'right' %}
				<!-- Container -->
				<div class="container">
					<div class="row">
						<!-- Page Content -->
						<div class="col-md-9">
							{{ page.content }}
						</div><!-- Column -->
						<!-- Sidebar -->
						{% if page.services_sidebar %}
							<div class="col-md-3">
								<aside class="sidebar">
									{{ page.services_sidebar }}
								</aside>
							</div><!-- Column -->
						{% endif %}
					</div><!-- Row -->
				</div><!-- Container -->
			{% else %}
				<!-- Container -->
				<div class="container">
					{{ page.content }}
				</div><!-- Container -->
			{% endif %}
		{% elseif services_layout == 'list' %}
			{% if services_sidebar == 'left' %}
				<!-- Container -->
				<div class="container">
					<div class="row">
						<!-- Sidebar -->
						{% if page.services_sidebar %}
							<div class="col-md-3">
								<aside class="sidebar">
									{{ page.services_sidebar }}
								</aside>
							</div><!-- Column -->
						{% endif %}
						
						<!-- Page Content -->
						<div class="col-md-9">
							<!-- List row -->
							<ul class="row">
								{{ page.content }}
							</ul><!-- List row -->
						</div><!-- Column -->
					</div><!-- Row -->
				</div><!-- Container -->
			{% elseif services_sidebar == 'right' %}
				<!-- Container -->
				<div class="container">
					<div class="row">
						<!-- Page Content -->
