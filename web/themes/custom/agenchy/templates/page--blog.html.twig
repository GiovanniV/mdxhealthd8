<div role="main" class="main">
	<div class="page-default bg-grey typo-dark">
		<!-- Container -->
			{% if blog_layout == 'grid' %}
				{% if blog_sidebar == 'left' %}
					<div class="container">
						<div class="row">
							{% if page.blog_sidebar %}
							<div class="col-md-3">
								<aside class="sidebar">
									{{ page.blog_sidebar }}
								</aside>
							</div>
							{% endif %}
							<div class="col-md-9">
								{{ page.content }}
							</div>
						</div>
					</div>
				{% elseif blog_sidebar == 'right' %}
					<div class="container">
						<div class="row">
							<div class="col-md-9">
								{{ page.content }}
							</div>
							{% if page.blog_sidebar %}
							<div class="col-md-3">
								<aside class="sidebar">
									{{ page.blog_sidebar }}
								</aside>
							</div>
							{% endif %}
						</div>
					</div>
				{% else %}
					<div class="container">
						{{ page.content }}
					</div>
				{% endif %}
			{% elseif blog_layout == 'list' %}
				{% if blog_sidebar == 'left' %}
					<div class="container">
						<div class="row">
							{% if page.blog_sidebar %}
							<div class="col-md-3">
								<aside class="sidebar">
									{{ page.blog_sidebar }}
								</aside>
							</div>
							{% endif %}
							<div class="col-md-9">
								<ul class="row">
									{{ page.content }}
								</ul><!-- Row -->
							</div><!-- Column -->
						</div>
					</div>
				{% elseif blog_sidebar == 'right' %}
					<div class="container">
						<div class="row">
							<div class="col-md-9">
								<ul class="row">
									{{ page.content }}
								</ul><!-- Row -->
							</div><!-- Column -->
							{% if page.blog_sidebar %}
							<div class="col-md-3">
								<aside class="sidebar">
									{{ page.blog_sidebar }}
								</aside>	
							</div>
							{% endif %}
						</div>
					</div>
				{% else %}
					<div class="container">
						<ul class="row">
							{{ page.content }}
						</ul><!-- Row -->
					</div>
				{% endif %}
