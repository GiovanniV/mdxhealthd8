									{{ page.blog_sidebar }}
								</aside>	
							</div>
							{% endif %}
							<!-- Page Content -->
							<div class="col-md-9">
								<ul class="row">
									{{ page.content }}
								</ul><!-- Row -->
							</div><!-- Column -->
						</div>
					</div>
				{% elseif blog_sidebar == 'right' %}
					<div class="container">
						<div class="row">
							<div class="col-md-9">
								<ul class="row">
									{{ page.content }}
