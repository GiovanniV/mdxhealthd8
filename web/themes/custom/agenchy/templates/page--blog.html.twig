						<div class="row">
							<div class="col-md-9">
								{{ page.content }}
							</div>
							{% if page.blog_sidebar %}
							<div class="col-md-3">
								<aside class="sidebar">
									{{ page.blog_sidebar }}
								</aside>
							</div>
							{% endif %}
						</div>
					</div>
				{% else %}
