							</div>
							{% endif %}
							<div class="col-md-9">
								{{ page.content }}
							</div>
						</div>
					</div>
				{% elseif blog_sidebar == 'right' %}
					<div class="container">
						<div class="row">
							<div class="col-md-9">
								{{ page.content }}
							</div>
							{% if page.blog_sidebar %}
							<div class="col-md-3">
								<aside class="sidebar">
									{{ page.blog_sidebar }}
								</aside>	
							</div>
							{% endif %}
						</div>
					</div>
				{% else %}
					<div class="container">
						{{ page.content }}
					</div>
				{% endif %}
			{% else %}
				{% if blog_sidebar == 'left' %}
					<div class="container case-sm-col">
						<div class="row">
							{% if page.blog_sidebar %}
							<div class="col-md-3">
								<aside class="sidebar">
									{{ page.blog_sidebar }}
								</aside>	
							</div>
							{% endif %}
							<div class="col-md-9">
								{{ page.content }}
							</div>
						</div>
					</div>
				{% elseif blog_sidebar == 'right' %}
