<!-- Theme Panel Switcher -->
{% if disable_switch =='on' %}
    {% include theme_path ~ "/templates/switcher-style.html.twig" %}
{% endif %}<!-- End Theme Panel Switcher -->

<!-- Header Begins -->
{% include theme_path ~ "/templates/header/header-3.html.twig" %}
<!-- Header Ends -->

<!-- Page Header -->
{% if page_headers_style == 'dark' %}
	{% include theme_path ~ "/templates/page header/page-header-2.html.twig" %}
{% elseif page_headers_style == 'small' %}
	{% include theme_path ~ "/templates/page header/page-header-3.html.twig" %}
{% elseif page_headers_style == 'center' %}
	{% include theme_path ~ "/templates/page header/page-header-4.html.twig" %}
{% elseif page_headers_style == 'button' %}
	{% include theme_path ~ "/templates/page header/page-header-5.html.twig" %}
{% elseif page_headers_style == 'contact' %}
	{% include theme_path ~ "/templates/page header/page-header-6.html.twig" %}
{% elseif page_headers_style == 'video' %}
	{% include theme_path ~ "/templates/page header/page-header-7.html.twig" %}
{% elseif page_headers_style == 'image' %}
	{% include theme_path ~ "/templates/page header/page-header-8.html.twig" %}
{% elseif page_headers_style == 'color' %}
	{% include theme_path ~ "/templates/page header/page-header-9.html.twig" %}
{% else %}
	{% include theme_path ~ "/templates/page header/page-header-1.html.twig" %}
{% endif %}
<!-- Page Header -->

<!-- Model Window -->
{% if page.model_window %}
	{{ page.model_window }}
{% endif %}
<!-- Model Window -->		
{% if header_style == 'style6' %}
	{% include theme_path ~ "/templates/header/header-6.html.twig" %}	
{% endif %}

{% set term_machine_name = current_term.vid.value.0.target_id %}
    {% if term_machine_name == 'blog_categories' %}
    	<div role="main" class="main">
			<div class="page-default bg-grey typo-dark">
				<div class="container">
					<div class="row">
						<div class="col-md-9">
							<ul class="row">
								{{ page.content_full_bar }}
								{{ page.content }}
							</ul><!-- Row -->
						</div><!-- Column -->
						{% if page.blog_sidebar %}
						<div class="col-md-3">
							{{ page.blog_sidebar }}	
						</div>
						{% endif %}
					</div>
				</div>
			</div><!-- Page Default -->
		</div><!-- Page Main -->
	{% elseif term_machine_name == 'tags' %}
		<div role="main" class="main">
			<div class="page-default bg-grey typo-dark">
				<div class="container">
					<div class="row">
						<div class="col-md-9">
							<ul class="row">
								{{ page.content_full_bar }}
								{{ page.content }}
							</ul><!-- Row -->
						</div><!-- Column -->
						{% if page.blog_sidebar %}
						<div class="col-md-3">
							{{ page.blog_sidebar }}	
						</div>
						{% endif %}
					</div>
				</div>
			</div><!-- Page Default -->
		</div><!-- Page Main -->
	{% elseif term_machine_name == 'cases_category' %}
		<div role="main" class="main">
			<div class="page-default bg-grey typo-dark">
				<div class="container">
					<div class="row">
						<!-- Page Content -->
						<div class="col-md-9">
							<ul class="row case-container case-list">
								{{ page.content }}
							</ul><!-- Row -->
						</div><!-- Column -->
						{% if page.case_sidebar %}
						<div class="col-md-3">
							{{ page.case_sidebar }}
						</div>
						{% endif %}
					</div><!-- Row -->		
				</div><!-- Container -->
			</div>
		</div>
	{% elseif term_machine_name == 'cases_tags' %}
		<div role="main" class="main">
			<div class="page-default bg-grey typo-dark">
				<div class="container">
					<div class="row">
						<!-- Page Content -->
						<div class="col-md-9">
							<ul class="row case-container case-list">
								{{ page.content }}
							</ul><!-- Row -->
						</div><!-- Column -->
						{% if page.case_sidebar %}
						<div class="col-md-3">
							{{ page.case_sidebar }}
						</div>
						{% endif %}
					</div><!-- Row -->		
				</div><!-- Container -->
			</div>
		</div>
	{% endif %}

<!-- Footer -->
{% include theme_path ~ "/templates/footer/footer-7.html.twig" %}
<!-- Footer -->