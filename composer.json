{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
      {
        "type": "composer",
        "url": "https://packages.drupal.org/8"
      },
      {
        "type": "package",
        "package": {
          "name": "harvesthq/chosen",
          "version": "1.6.1",
          "type": "drupal-library",
          "source": {
            "url": "https://github.com/harvesthq/chosen",
            "type": "git",
            "reference": "v1.6.1"
          },
          "require": {
            "composer/installers": "^1.2.0"
          }
        }
      }
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal-composer/drupal-scaffold": "^2.2",
        "drupal/admin_toolbar": "^1.23",
        "drupal/allowed_formats": "^1.1",
        "drupal/backup_migrate": "^4.0@RC",
        "drupal/better_exposed_filters": "^3.0@alpha",
        "drupal/block_visibility_groups": "^1.2",
        "drupal/blocktabs": "^1.0@alpha",
        "drupal/chosen": "^2.5",
        "drupal/console": "^1.0.2",
        "drupal/contact_block": "^1.4",
        "drupal/contact_storage": "^1.0@beta",
        "drupal/core": "~8.5",
        "drupal/ctools": "^3.0",
        "drupal/devel": "^1.2",
        "drupal/dropzonejs": "^2.0@alpha",
        "drupal/embed": "^1.0",
        "drupal/entity_browser": "^1.4",
        "drupal/entity_embed": "^1.0@beta",
        "drupal/field_group": "^1.0",
        "drupal/file_browser": "^1.1",
        "drupal/get_tweets": "^1.1",
        "drupal/globalredirect": "1.x-dev",
        "drupal/google_analytics": "^2.2",
        "drupal/google_analytics_reports": "^3.0@beta",
        "drupal/image_raw_formatter": "1.x-dev",
        "drupal/image_url_formatter": "^1.0@beta",
        "drupal/imagemagick": "~2.0",
        "drupal/lang_dropdown": "^2.0@beta",
        "drupal/languageicons": "^1.0@beta",
        "drupal/menu_attributes": "1.x-dev",
        "drupal/metatag": "^1.5",
        "drupal/pathauto": "1.0",
        "drupal/pdfpreview": "^1.0@RC",
        "drupal/php": "^1.0@beta",
        "drupal/redirect": "^1.1",
        "drupal/robotstxt": "^1.1",
        "drupal/seo_checklist": "^4.1",
        "drupal/simple_sitemap": "^2.12",
        "drupal/simplenews": "^1.0@alpha",
        "drupal/sitemap": "^1.3",
        "drupal/soundcloudfield": "^1.0@alpha",
        "drupal/term_condition": "^1.1",
        "drupal/token": "^1.1",
        "drupal/twitter_block": "^2.1",
        "drupal/twitter_feed": "^1.2",
        "drupal/ubercart": "^4.0@alpha",
        "drupal/ultimate_cron": "^2.0@alpha",
        "drupal/video_embed_field": "^1.5",
        "drupal/views_load_more": "1.x-dev",
        "drupal/xmlsitemap": "^1.0@alpha",
        "drupal/yamlform": "^1.0@beta",
        "drush/drush": "^9.0.0",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        },
        "patches": {
            "drupal/languageicons": {
                "Call to undefined method id": "https://www.drupal.org/files/issues/call-to-undefined-method-id-2932988-1_0.patch"     
            }
        }
    }
}
